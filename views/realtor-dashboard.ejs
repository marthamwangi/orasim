                showInLegend: true,
                name: "Listings",
                lineDashType: "dash",
                dataPoints: [
                  { x: new Date(2022, 0, 3), y: 510 },
                  { x: new Date(2022, 0, 4), y: 560 },
                  { x: new Date(2022, 0, 5), y: 540 },
                  { x: new Date(2022, 0, 6), y: 558 },
                  { x: new Date(2022, 0, 7), y: 544 },
                  { x: new Date(2022, 0, 8), y: 693 },
