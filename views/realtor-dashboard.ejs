                title: "Listings",
                includeZero: true,
                crosshair: {
                  enabled: true
                }
              },
              toolTip: {
                shared: true
              },
              legend: {
                cursor: "pointer",
                verticalAlign: "bottom",
                horizontalAlign: "left",
                dockInsidePlotArea: true,
                itemclick: toogleDataSeries
              },
              data: [{
                type: "line",
                showInLegend: true,
                name: "Clients",
                markerType: "square",
                xValueFormatString: "DD MMM, YYYY",
